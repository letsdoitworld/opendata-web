{
  "name": "ldiw-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "carto.js": "^4.0.0-beta.41",
    "chroma-js": "^1.3.4",
    "classnames": "^2.2.5",
    "dateformat": "^2.0.0",
    "jquery": "^3.2.1",
    "leaflet": "^1.2.0",
    "moment": "^2.21.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.0",
    "react": "^16.2.0",
    "react-country-flag": "^0.2.0",
    "react-dom": "^16.0.0",
    "react-ga": "^2.3.5",
    "react-leaflet": "1.7.8",
    "react-leaflet-choropleth": "^2.0.0",
    "react-leaflet-control": "^1.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.11",
    "react-types": "^0.1.0",
    "vis": "^4.20.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.3"
  }
}
